<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <WebView
            android:layout_width="391dp"
            android:layout_height="378dp"
            android:id="@+id/web1" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button2" app:layout_constraintVertical_bias="0.402"/>
    <Button
            android:text="2222"
            android:layout_width="90dp"
            android:layout_height="50dp" android:id="@+id/button1"
            app:layout_constraintEnd_toStartOf="@+id/button2"
            android:layout_marginEnd="24dp" app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
            android:layout_marginTop="20dp"/>
    <Button
            android:text="Button"
            android:layout_width="90dp"
            android:layout_height="50dp" android:id="@+id/button2"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="144dp"/>
    <Button
            android:text="3333"
            android:layout_width="90dp"
            android:layout_height="50dp" android:id="@+id/button3"
            app:layout_constraintStart_toEndOf="@+id/button2"
            android:layout_marginStart="20dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"/>
    <EditText
            android:layout_width="342dp"
            android:layout_height="78dp"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editTextTextPersonName2"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>